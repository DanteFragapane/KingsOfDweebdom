  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mysql": "^2.17.1"
