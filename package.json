  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanteFragapane/KingsOfDweebdom/issues"
  },
  "homepage": "https://github.com/DanteFragapane/KingsOfDweebdom#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "dotenv": "^8.1.0",
    "nodemon": "^1.19.3",
    "restart": "^2.0.0"
  },
  "dependencies": {
    "@restart/context": "^2.1.4",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.3",
    "fastidious-envelope-generator": "^2.0.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "react": "^16.10.2",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.0.1"
  }
}
